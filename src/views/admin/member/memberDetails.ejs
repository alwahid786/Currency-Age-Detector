                        <br>
                        <div class="box-heading">
                           <h2 class="m-t-md m-b-md">KYC Documents</h2>     	
                             </div>
                             <% if(data.map(x=>x.kycDocument).some(e=>e === undefined)){%>
                              <p>Not available</p>
                              <%}else if(data.map(x=>x.kycDocument).length <= 1){%>
                                 <p>Not available</p>
                              <%}else{%>
                             <div class="row" id="kyc-doc">
                                 <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                     <div class="box">
                             
                        <%if(data.map(x=>x.kycDocument.map(x=>x.document.url)).length > 3){%> 

                        <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[0])[0]%>" alt="" class="myImg" >
                        <%}else{%>
                        <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[0])%>" alt="" class="myImg" >
                       <%}%>
                        <i class="fa fa-search-plus" aria-hidden="true"></i>
                   
                      </div>
                    
                                 </div>
                                     <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                        <div class="box">
                                     
                         <%if(data.map(x=>x.kycDocument.map(x=>x.document.url)).length > 3){%>               
                                                     
                        <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[1])[1]%>" alt="" class="myImg" >
                        <%}else{%>
                           <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[1])%>" alt="" class="myImg" >
                           <%}%>
                        <i class="fa fa-search-plus" aria-hidden="true"></i>
                   
                      </div>
                       
                                 </div>
                                     <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                      <div class="box">
                                         
                                             
                                       <%if(data.map(x=>x.kycDocument.map(x=>x.document.url)).length > 3){%> 
                                          <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[2])[2]%>" alt="" class="myImg" >
                                          <%}else{%>                         
                        <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[2])%>" alt="" class="myImg" >
                        <%}%>
                        <i class="fa fa-search-plus" aria-hidden="true"></i>
                        
                     </div>
                     
                  </div>
                  <%}%>
                                 
                             </div>
                     </div>

                  </div>
               </div>
