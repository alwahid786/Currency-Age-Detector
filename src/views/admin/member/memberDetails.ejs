                                          <thead>
                                             <tr>
                                                <th>Coin Name</th>
                                                <th>Details</th>
                                                <th>Grading Status</th>                                               
                                                <th>Auction</th>
                                                <th>Price</th>
                                                <th>Action</th>
                                             </tr>
                                          </thead>
                                          <tbody>
                                             <%totalCoins.forEach(function(coin){%>
                                                <% if(typeof coin == 'undefined'){ %>
                                                   <%}else{%>
                                                <tr>
                                                   <td>
                                                      <%= coin.name%>
                                                   </td>
                                                   <td>
                                                      <%= coin.history.slice(0, 8)%>...
                                                   </td>
                                                   <%if(coin.isGraded == true){%>
                                                   <td>
                                                     Graded
                                                   </td>
                                                  <%}else{%>
                                                   <td>
                                                     Not Graded
                                                    </td>
                                                    <%}%>
                                                   <% if(coin.marketPlaceState=='ON_AUCTION' ){%>
                                                      <td>Yes</td>
                                                      <%}else{%>
                                                         <td>No</td>
                                                         <%}%>
                                                            <td>
                                                               <%= coin.price%>
                                                            </td>
                                                            <td>
                                                               <div class="action-btn">
                                                                  <a style="margin-left: 13px;"  href="admin/coin/<%= coin._id%>"><i
                                                                        class="fa fa-eye" aria-hidden="true"></i></a>
                                                               </div>
                                                            </td>
                                                </tr>
                                                
                                                <%}%>
                                                <%})%>
                                          </tbody>
                                       </table>
                                    </div>
                                 </div>
