            <div class="wrapper wrapper-content">

                <div class="row">
                      <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                             <div class="ibox">
                                     <div class="ibox-content" id="history">
                                         <div class="box-heading">
                     <a href="admin/kycRequests">
                       <h2> <img src="img/back-icon.png" class="img-back">KYC Request Details
                       </h2>
                     </a>

                   </div>
         
                                         <div class="border-solid m-b-lg">
                                     <div class="text-right m-b-md">
                                        <% if(data.map(x=>x.kyc.isKyc) == 'verified'){%>
                                          <a href="#reject-modal" class="btn btn-default min-135 btn-small"  data-toggle="modal">Reject</a>
                                          <%}else if(data.map(x=>x.kyc.isKyc) == 'reject'){%>
                                             <a class="btn btn-default min-135 btn-small disabled " >Rejected</a>
                                             <%}else{%>
                                             <a href="#" class="btn btn-default min-135 btn-small" data-toggle="modal" data-href="admin/approveKyc/<%= data.map(x=>x._id)%>" data-target="#modal-approve"> Approve</a>
                                          <a href="#reject-modal" class="btn btn-default min-135 btn-small"  data-toggle="modal">Reject</a>
                                          <%}%>
                                                           </div>  
                                                           <div class="row">
                             <div class="col-md-12">
                                 <div class="row ">
                                     <% data.forEach(item=>{%>
                                   <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="detail-title-a">
                                        <p><span>First Name:</span><%= item.firstName%></p>
                                            <p><span>Last Name:</span><%= item.lastName%></p>
                                             <p><span>DOB:</span><%= item.dob.slice(0, 10)%></p>
                                         <p><span>Email Address:</span><%= item.email%></p>
                                       <p><span>Phone Number:</span><%= item.phone%></p>
                                    <p><span>Document Type: <% if(typeof item.kycDocument == 'undefined'){%>

                                    <%}else{%></span><%= item.kycDocument.map(x=>x.type)%> </p>
                                      <%}%>
                                     
                                    </div>
                                    </div>
                                 <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                     <div class="detail-title-a">
                                      
                                      
                                       <p><span>Counrty:</span><%= item.country%></p>
                                       
                                       <p><span>City:</span><%= item.city%></p>
                                      
                                       <% if(item.kyc.isKyc == 'verified'){%>
                                        <p><span>KYC Status:</span>Approved</p>
                                        <%}else if(item.kyc.isKyc == 'reject'){%>
                                        <p><span>KYC Status:</span>Rejected</p>
                                        <%}else{%>
                                          <p><span>KYC Status:</span>Pending</p>
                                          <%}%>
                                         <p><span>Date:</span><%=item.created.toDateString().substr(3, 12)%></p>
                                         <% if(item.kyc.kycRejectReason == 'null'){%>
                                          <p><span>Reason:</span></p>
                                          <%}else{%>
                                          <p><span>Reason:</span><%= item.kyc.kycRejectReason%></p>
                                             <%}%>
                                      
                                    </div>
                                  </div>
                                  <%})%>
                              </div>
                            </div>
                        </div>    
                        </div> 
   
                                           
                                              
                                           
                                
                                     <!-- kyc documents -->
                                            <div class="box-heading">
                                          <h2 class="m-t-md m-b-md">KYC Documents</h2>     	
                                            </div>
                                            <div class="row" id="kyc-doc">
                                               <% if(data.map(x=>x.kycDocument).some(e=>e === undefined)){%>
                                                <p style="margin-left: 17px;">Not available</p>
                                                <%}else if(data.map(x=>x.kycDocument.map(x=>x.document)) == ''){%>
                                                   <p style="margin-left: 17px;">Not available</p>
                                                <%}else{%>
                                                <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                                    <div class="box">
                                       <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[0])%>" alt="" class="myImg" >
                                     
                                       <i class="fa fa-search-plus" aria-hidden="true"></i>
                                  
                                     </div>
                                   
                                                </div>
                                                    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                                  <div class="box">
                                                   
                                       <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[1])%>" alt="" class="myImg" >
                                      
                                       <i class="fa fa-search-plus" aria-hidden="true"></i>
                                  
                                     </div>
                                      
                                                </div>
                                                    <div class="col-lg-3 col-md-4 col-sm-6 col-xs-12" >
                                                     <div class="box">
                                                        
                                       <img src="<%= data.map(x=>x.kycDocument.map(x=>x.document.url)[2])%>" alt="" class="myImg" >
                                       <i class="fa fa-search-plus" aria-hidden="true"></i>
                                       
                                    </div>
                                    
                                 </div>
                                 <%}%>
                                                
                                            </div>
                                             
                                     </div>
                             </div>
                      </div>
                </div>
                
                
           </div>
