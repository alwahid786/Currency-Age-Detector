      <div class="modal coman-modal fade" id="modal-approve" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
        <div class="modal-content text-center">
            <h3 style="font-size: 20px;
            color: #000;
            margin-bottom: 20px;">Are you sure you want to approve?</h3>

            <div class="clearfix">
               <div style="display: flex; align-items: center; justify-content: center; ">
                  <button style="padding: 7px 0px;
                  border-radius: 5px;
                  max-width: 100px; width: 100%; margin-right: 10px;" type="submit" class="btn   btn-default m-r-sm" data-dismiss="modal" >No</button>
                  <button style="padding: 7px 0px;
                  border-radius: 5px;
                  max-width: 100px; width: 100%;" type="submit" class="btn btn-primary approveKyc" data-dismiss="modal"  data-toggle="modal" data-target="#modal-successa">Yes</button>

               </div>
            </div>
            
        </div>
        </div>
    
      </div>
      <div class="modal coman-modal  fade" id="modal-successa" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
        <div class="modal-dialog" role="document">
           <div class="modal-content text-center">
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
              </button>
              <div class="success-msg-icon">
                 <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
              </div> 
              <h2>Approved successfully !!</h2>
           </div>
        </div>
     </div>
          <div class="modal coman-modal  fade" id="modal-successa" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
          <div class="modal-dialog" role="document">
             <div class="modal-content text-center">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
                <div class="success-msg-icon">
                   <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
                </div> 
                <h2>Deleted successfully !!</h2>
             </div>
          </div>
       </div>
      <div class="modal coman-modal  fade" id="Status-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
         <div class="modal-dialog modal-sm modal-a" role="document">
            <div class="modal-content text-center">
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
               <span aria-hidden="true">&times;</span>
               </button>
               <h2>Status</h2>
               <div class="form-group text-left">
                  <label> <span>Document 1 :</span> Approve</label>
               </div>
               <div class="form-group text-left">
                  <label><span>Document 2 : </span> Reject Image is Blur</label>
               </div>
               <div class="form-group  text-left">
                  <label><span>Document 3 : </span> Approve</label>
               </div>
            </div>
         </div>
      </div>
      <!-- IMAGE MODAL -->
      <div id="myModal" class="modal kyc myModal">
         <span class="close cursor" onclick="closeModal()">&times;</span>
         <img class="modal-content-A img01" >
         <!-- <div id="caption"></div> -->
      </div>
      <div class="modal coman-modal  fade" id="cong-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
         <div class="modal-dialog  modal-a" role="document">
            <div class="modal-content text-center">
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
               <span aria-hidden="true">&times;</span>
               </button>
               <div class="success-msg-icon">
                  <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
               </div>
               <h2>Vendor  successfully  Rejected!!</h2>
            </div>
         </div>
      </div>
      <div class="modal coman-modal  fade" id="reject-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
         <div class="modal-dialog  modal-a" role="document">
            <div class="modal-content text-center">
               <button type="button" class="close" data-dismiss="modal" aria-label="Close">
               <span aria-hidden="true">&times;</span>
               </button>
               <h2>Reason</h2>
               <form action="admin/rejectKyc/<%=data.map(x=>x._id)%>?_method=PUT" method="POST">
               <div class="form-group">
                  <textarea class="form-control"  name="kycRejectReason" value="" placeholder="Enter the Reason of Rejection"></textarea>
               </div>
               <button type="submit" class="btn m-r-sm btn-primary btn-small max-83" >Submit</button>
               </form>
            </div>
         </div>
      </div>
     
      <!-- Mainly scripts -->
      <script src="js/jquery-2.1.1.js"></script>
      <script src="js/inspinia.js"></script>
      <script src="js/bootstrap.min.js"></script>
