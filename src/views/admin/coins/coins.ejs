            <div class="wrapper wrapper-content">
                <% if(data ==''){%>
                    <h2>Coin data not found</h2>
                    <%}%>
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="ibox">
                            <div class="ibox-content" id="history">
                                <div class="box-heading">
                                    <h2>Coins</h2>
                                </div>
                               
                                <!-- <div class="text-right m-b-sm"> <button type="submit" class="btn btn-primary"
                                        data-toggle="modal" data-target="#Add-Coins">Add Coins</button></div> -->
                                <div class="table-responsive coman-table" id="owner-detail-btn">
                                    <table class="table dataTables-example-coin">
                                        <thead>
                                            <tr>

                                                
                                                <th>Coin Name</th>
                                                <th>Price range</th>
                                                <th>Ruler</th>
                                                <th>Year</th>
                                                <th>Uploaded By</th>
                                                <th>Actions</th>

                                            </tr>
                                        </thead>
                                        <tbody>
                                        <% data.forEach(function(coin){%>
                                            <tr>
                                               
                                                <td><%= coin.name%></td>
                                                <td>100-200</td>
                                                <td><%= coin.ruler%></td>
                                                <td><%= coin.year%></td>
                                                <td><%= coin.userDetails.firstName%></td>                                               
                                                <td>
                                                    <div class="action-btn">

                                                        <a data-toggle="tooltip" data-placement="top" title="View" href="admin/coin/<%= coin._id%>"><i class="fa fa-eye" aria-hidden="true"></i></a>
                            
                                                            <a href=""  id="coinId" data-href="admin/delete-coin/<%= coin._id%>" data-toggle="modal" data-target="#modal-delete"><i
                                                                    class="fa fa-trash" aria-hidden="true"></i></a>
                                                    </div>
                                                 </td>
                                            </tr>
                                        <%})%>   

                                        </tbody>
                                    </table>
                                   
                                </div>

                            </div>
