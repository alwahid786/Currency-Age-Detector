                  <textarea class="form-control" placeholder="Enter the Reason of Rejection"></textarea>
               </div>
               <a href="#cong-modal" class="btn m-r-sm btn-primary btn-small max-83" data-dismiss="modal" data-toggle="modal">Submit</a>
            </div>
         </div>
