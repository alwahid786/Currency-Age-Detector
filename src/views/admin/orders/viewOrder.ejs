            <div class="wrapper wrapper-content">
               <div class="row">
                  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                     <div class="ibox">
                        <div class="ibox-content" id="history">
                           <div class="box-heading">
                              <a href="admin/orders">
                                 <h2> <img src="img/back-icon.png" class="img-back">Order Details
                                 </h2>
                              </a>
                           </div>
                           <div class="border-solid m-b-lg">
                              <% if(data.map(x=>x.coinDetails).some(e=>e === undefined)){ %>
                                 <h4>Coin details not found</h4>
                                 <%}else{%> 
                              <div class="row">
                                 <div class="col-md-12">
                                    <div class="row ">
                                       <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                                            
                                          <img src="<%=data.map(x=>x.coinDetails.pictures.front.url)%>"  class="profile-img-b profile-img-a" >
                                          <h4 class="text-center">Front View</h4>
                                          
                                       </div>
                                        <div class="col-lg-2 col-md-3 col-sm-6 col-xs-12">
                                         
                                          <img src="<%=data.map(x=>x.coinDetails.pictures.back.url)%>"  class="profile-img-b profile-img-a" >
                                           <h4 class="text-center">Back View</h4>
                                       </div>
                                        <div class="col-lg-7 col-md-6 col-sm-6 col-xs-12 float-right">
                                            <h2>Coin Information:</h2>
                                          <div class="box-walleta table-responsive">
                                           <table id="customers">
                                                <thead>
                                                   <tr>
                                                      <td>Coin name:</td>
                                                      <td><%=data.map(x=>x.coinDetails.name)%></td>
                                                   </tr>
                                                   <tr>
                                                      <td>Country:</td>
                                                      <td><%=data.map(x=>x.coinDetails.country)%></td>
                                                   </tr>
                                                   <tr>
                                                      <td>Year:</td>
                                                      <td><%=data.map(x=>x.coinDetails.year)%></td>
                                                   </tr>
                                                   <tr>
                                                     <td>Grad:</td>
                                                      <% if(data.map(x=>x.coinDetails.gradingMetadata).some(e=>e===undefined)){%>
                                                         <td></td>
                                                         <%}else{%>
                                                         <td ><%=data.map(x=>x.coinDetails.gradingMetadata.value)%></td>
                                                            <%}%>
                                                      
                                                   </tr>
                                                   
                                              
                                                     <tr>
                                                      <td>Weight:</td>
                                                      <td ><%=data.map(x=>x.coinDetails.weight)%></td>
                                                   </tr>
                                                     <tr>
                                                      <td>Diameter:</td>
                                                      <td ><%=data.map(x=>x.coinDetails.diameter)%></td>
                                                   </tr>
                                                      <tr>
                                                      <td>Thickness:</td>
                                                      <td ><%=data.map(x=>x.coinDetails.thickness)%></td>
                                                   </tr>
                                                    <tr>
                                                      <td>Shape:</td>
                                                      <td ><%=data.map(x=>x.coinDetails.shape)%></td>
                                                   </tr>
                                                    <tr>
                                                      <td>Material:</td>
                                                      <td ><%=data.map(x=>x.coinDetails.material)%></td>
                                                   </tr>  
                                                    
                                                   <tr>
                                                      <td>price:</td>
                                                      <td ><%=data.map(x=>x.coinDetails.price)%></td>
                                                   </tr>    
                                                </thead>
                                             </table>
                                          </div>
                                        
                                        
                                       </div>
                                  
                                    </div>
