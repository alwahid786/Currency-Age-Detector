                     <div class="row justify-content-between d-flexa">
                        <%if(data.map(x=>x.trackMeta).some(e=>e === undefined)){%>
                           <p>Tracking data not available</p>
                        <%}else{%>
                           <div class="order-tracking completed">
                              <span class="is-complete"></span>
                              <p>Tracking Id<br><span><%= data.map(x=>x.trackMeta.trackId)%></span></p>
                           </div>
                           <div class="order-tracking completed">
                              <span class="is-complete"></span>
                              <p>Website<br>  <a href="<%= data.map(x=>x.trackMeta.website)%>"> <span><%= data.map(x=>x.trackMeta.website)%></a></span></p>
                           </div>
                           <div class="order-tracking completed">
                              <span class="is-complete"></span>
                              <p>Name<br><span><%= data.map(x=>x.trackMeta.name)%></span></p>
                           </div>
                           <div class="order-tracking completed">
                              <span class="is-complete"></span>
                              <p>Order on<br><span><%= data.map(x=>x.createdAt.toDateString())%></span></p>
                           </div>

                        <%}%>
