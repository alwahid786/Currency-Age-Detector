      tabindex="-1"
      role="dialog"
      aria-labelledby="myModalLabel"
    >
      <div class="modal-dialog" role="document">
        <div class="modal-content text-center">
          <h3>Are you sure you want to Delete Bank Note?</h3>
          <div class="clearfix">
            <button
              type="submit"
              class="btn btn-default m-r-sm"
              data-dismiss="modal"
            >
              No
            </button>
            <button
              type="submit"
              data-toggle="modal"
              class="btn btn-primary deleteBankNote"
              data-dismiss="modal"
              data-target="#modal-successa"
            >
              Yes
            </button>
          </div>
        </div>
      </div>
