        <div class="wrapper wrapper-content">
            
          <div class="row">
              
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
              <div class="ibox">
                <div class="ibox-content" id="history">
                  <div class="box-heading">
                    <h2>Bank Note</h2>
                  </div>

                  <!-- <div class="text-right m-b-sm">  <button type="submit" class="btn btn-primary"   data-toggle="modal" data-target="#Add-Coins">Add Notes</button></div> -->

                  <div
                    class="table-responsive coman-table"
                    id="owner-detail-btn"
                  >
                    <table class="table dataTables-note">
                        
                      <thead>
                        <tr>
                          <th>ID</th>
                          <th>Note Name</th>
                          <th>History</th>
                          <th>Price range for graded</th>
                          <th>Price range for non-graded</th>
                          <th>Ruler</th>
                          <th>Year</th>
                          <th>Uploaded By</th>
                          <th>Actions</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% data.forEach(function(note){%>
                        <tr>
                          <td><%= note._id%> </td>
                          <td><%= note.name%></td>
                          <td><%= note.history.slice(0, 8)%>...</td>
                          <td>100-200</td>
                          <td>300-350</td>
                          <td><%= note.ruler%></td>
                          <td><%= note.year%></td>
                          <td><%= note.userDetails.firstName%></td>

                          <td>
                            <div class="action-btn">
                              <a data-toggle="tooltip" data-placement="top" title="View" href="admin/bankNote/<%= note._id %>"
                                ><i class="fa fa-eye" aria-hidden="true"></i
                              ></a>
                              <a href="#" data-href="admin/delete/bankNote/<%= note._id%>" data-toggle="modal" data-target="#modal-delete"><i class="fa fa-trash" aria-hidden="true"></i></a>
                            </div>
                          </td>
                        </tr>
                        <%})%>     
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
