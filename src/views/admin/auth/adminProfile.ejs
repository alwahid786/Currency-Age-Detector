    <script src="js/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="js/plugins/slimscroll/jquery.slimscroll.min.js"></script>

    <!-- Custom and plugin javascript -->
    <script src="js/plugins/pace/pace.min.js"></script>



    <div class="modal fade coman-modal" id="modal-change-pw" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <form role="form" id="updatepassword">
            
            <div class="modal-content text-center">
                <div>
                    <button type="button" class="close modal-close" data-dismiss="modal">&times;</button>
                </div>
                <h2> Change Password</h2>

                <div class="form-group pw-form">
                    <input id="oldPassword" type="password" name="oldPassword" class="form-control" placeholder="Old Password" required=true value="">
                </div>
                <div class="form-group pw-form">
                        <input id="newPassword" type="password" name="newPassword" class="form-control" placeholder="New Password" value="">
                    </div>
                    <div class="form-group pw-form">
                        <input id="confirmPassword" type="password" name="confirmPassword" class="form-control" placeholder="Confirm New Password" value="">
                    </div>
                    <button type="button"  class="btn btn-primary updatePassword" data-toggle="modal" data-dismiss="modal"
                    data-target="#modal-success">Submit</button>
                    
                    
                    
                </form>
                    </div>
                </div>
            </div>
    <div class="modal coman-modal  fade" id="modal-success" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content text-center">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <div class="success-msg-icon">
                    <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
                </div>
                <% if(error) { %>
                    <h2 class="alert alert-danger" role="alert"><strong><%= error %></strong></h2>
                   <% }else{ %>
                <h2>Updated successfully !!</h2>
                <%}%>
            </div>
        </div>
    </div>

</script>
