        <div id="page-wrapper" class="dark-bg">
            <div class="row top-menu2">
                <nav class="navbar navbar-static-top dark-bg-header top-menu" role="navigation"
                    style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary "><i class="fa fa-bars"></i>
                        </a>
                     </div>
                    <ul class="nav navbar-top-links navbar-right">
                        <li class="user-info-box welcome tooltipa">
                            <div class="dropdown d-flex">
                                <span class="prof-img">
                                    <a href="admin/profile"> 
                                        <% if( data.map(x=>x.profilePic).some(element => element === undefined)){ %>
                                        <img alt="image" class="img-back" src="img/prf-img.png"></a>
                                        <% }else{ %>
                                        <img alt="image" class="img-back" src="<%=data.map(x=>x.profilePic.url)%>"></a>
