            <div class="wrapper wrapper-content">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="ibox">
                            <div class="ibox-content">
                                <div class="box-heading">
                                    <div class="row">
                                        <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                                            <h2>My Account</h2>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                            <% if(error) { %>
                                                <div class="alert alert-danger alert-dismissible " role="alert">
                                                  <strong><%=error%></strong> 
                                                  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                  </button>
                                                </div>
                                              <% } %>
                                              <% if(success) { %>
                                                <div class="alert alert-success alert-dismissible " role="alert">
                                                  <strong><%=success%></strong>
                                                  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                  </button>
                                                </div>
                                               <% } %>
                                        </div>
                                    </div>

                                </div>

                                <div class="my-ac-section">
                                    <div class="my-ac-img">
                                        <% if(data.map(x=>x.profilePic).some(element => element === undefined)){ %>
                                        <img src="img/prf-img.png" alt="">
                                        <% }else{ %>
                                        <img id="profile" src="<%= data.map(x=>x.profilePic.url)%>" alt="">
                                        <%}%>
                                        <form action="admin/profile/uploadProfilePic?_method=PUT" method="POST" enctype="multipart/form-data">
                                        <div class="browse-btn" >
                                                <span><i class="fa fa-camera" aria-hidden="true"></i></span>
                                                <input type="file"  name="file" accept="image/*" id="uploadPic">
                                             
                                         </div>
                                         <br>
                                         <div>
                                            
                                            <button type="submit"  class="btn btn-sm btn-primary" >Upload  </button>
                                         </div>
                                           

                                           
                                        </form>
                                    </div>
                                    <div id="">
                                        <div class="form-group usr-form">
                                            <i class="fa fa-user" aria-hidden="true"></i>
                                            <input disabled="" type="text" name="" placeholder="First Name" class="form-control"
                                                value="<%=data.map(x=>x.firstName)%>">
                                        </div>
                                        <div class="form-group usr-form">
                                            <i class="fa fa-user" aria-hidden="true"></i>
                                            <input disabled="" type="text" placeholder="Last Name" class="form-control"
                                                value="<%=data.map(x=>x.lastName)%>">
                                        </div>
                                        <div class="form-group ph-form">
                                            <i class="fa fa-phone" aria-hidden="true"></i>
                                            <input disabled="" type="text" placeholder="Phone" class="form-control"
                                                value="<%=data.map(x=>x.countryCode)%><%=data.map(x=>x.phone)%>">
                                        </div>
                                        <div class="form-group eml-form">
                                            <i class="fa fa-envelope" aria-hidden="true"></i>
                                            <input type="text" placeholder="Email" class="form-control" disabled=""
                                                value="<%=data.map(x=>x.email)%>">
                                        </div>
                                        <div class="clearfix change-pw">

                                            <a href="admin/update-password"><i
                                                    class="fa fa-lock" aria-hidden="true" style="position: relative; display: inline-block; top: 1px;
    left: -4px;"></i> Change Password</a>
                                        </div>
                                        <div class="clearfix">
                                            <button type="submit"  class="btn btn-primary btn-block" data-toggle="modal"
                                                data-target="#claimnowa">Update</button>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>


            </div>
